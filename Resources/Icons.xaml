<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Image" x:Key="TitleBarIconStyle">
        <Setter Property="Width" Value="16"/>
        <Setter Property="Height" Value="16"/>
        <Setter Property="Margin" Value="0 1 0 0"/>
    </Style>
    <Image x:Key="MinimizeIconImg" Style="{StaticResource TitleBarIconStyle}" Source="../../UI/Icons/minimize-icon.png"/>
    <Image x:Key="MaximizeIconImg" Style="{StaticResource TitleBarIconStyle}" Source="../../UI/Icons/maximize-icon.png"/>
    <Image x:Key="CloseIconImg" Style="{StaticResource TitleBarIconStyle}" Source="../../UI/Icons/close-icon.png"/>
    <Image x:Key="RoyalCarsIconImg" Style="{StaticResource TitleBarIconStyle}" Source="../../UI/Icons/royal-cars-icon.png"/>
    <Image x:Key="RestoreIconImg" Style="{StaticResource TitleBarIconStyle}" Source="../../UI/Icons/restore-icon.png"/>
</ResourceDictionary>